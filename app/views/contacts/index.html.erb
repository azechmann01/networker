<div class="page-header">
  <h1>
    Contacts
    <a href="/contacts/new" class="btn btn-lg btn-default">New Contact</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Company</th>
        <th>Actions</th>


        </tr>

      <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.company_id %></td>
        <td>
          <a href="/contacts/<%= contact.id %>" class="btn btn-primary">Profile</a>
          <a href="/contacts/<%= contact.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_contact/<%= contact.id %>" class="btn btn-danger" rel="nofollow">Delete</a>

      </tr>
      <% end %>
    </table>
  </div>
</div>
