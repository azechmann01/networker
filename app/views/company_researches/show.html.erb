<div class="page-header">
  <h3>Company Research #<%= @company_research.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Article link</dt>
      <dd><%= @company_research.article_link %></dd>

      <dt>Company</dt>
      <dd><%= @company_research.company_id %></dd>

      <dt>User</dt>
      <dd><%= @company_research.user_id %></dd>

    </dl>



    <div class="btn-group btn-group-justified">
      <a href="/company_researches" class="btn btn-primary">
        Back
      </a>
      <a href="/company_researches/<%= @company_research.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_company_research/<%= @company_research.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
