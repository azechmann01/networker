<div class="page-header">
  <h1>
    Company Research
    <a href="/company_researches/new" class="btn btn-lg btn-success">Add New Article</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Article link</th>
        <th>Company</th>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @company_researches.each do |company_research| %>
      <tr>
        <td><%= company_research.article_link %></td>
        <td><%= company_research.company_id %></td>
        <td><%= company_research.user_id %></td>
        <td>
          <a href="/company_researches/<%= company_research.id %>" class="btn btn-primary">Show</a>
          <a href="/company_researches/<%= company_research.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_company_research/<%= company_research.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
